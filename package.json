{
    "name": "todo-list",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "s": "vue-cli-service serve",
        "b": "vue-cli-service build",
        "l": "vue-cli-service lint",
        "eb-m": "vue-cli-service electron:build --mac --config electron-builder.json",
        "eb-w": "vue-cli-service electron:build --win --x64 --config electron-builder.json",
        "es": "vue-cli-service electron:serve",
        "sqlite-rebuild": "node-gyp rebuild --target=1.3.1 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.3-win32-x64",
        "postinstall": "electron-builder install-app-deps",
        "pack:win": "build --win --x64",
        "test": "jest"
    },
    "jest": {
        "collectCoverageFrom": ["**/*.{js,vue}", "!**/node_modules/**"],
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest"
        }
    },
    "main": "background.js",
    "dependencies": {
        "@babel/polyfill": "^7.4.4",
        "animate.css": "^4.1.0",
        "core-js": "^3.6.4",
        "electron-rebuild": "^1.11.0",
        "raw-loader": "^4.0.1",
        "sqlite3": "^5.0.0",
        "vue": "^2.6.11",
        "vue-jest": "^3.0.6",
        "vue-router": "^3.3.4",
        "vuetify": "^2.2.11",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.3.0",
        "@vue/cli-plugin-eslint": "~4.3.0",
        "@vue/cli-service": "~4.3.0",
        "@vue/test-utils": "^1.0.4",
        "babel-eslint": "^10.1.0",
        "electron": "^9.0.0",
        "electron-builder": "^22.8.0",
        "electron-devtools-installer": "^3.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^26.4.2",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.2",
        "spectron": "^11.1.0",
        "vue-cli-plugin-electron-builder": "^2.0.0-rc.4",
        "vue-cli-plugin-vuetify": "~2.0.5",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie <= 10"
    ]
}